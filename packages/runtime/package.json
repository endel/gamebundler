{
  "name": "@gamebundler/runtime",
  "version": "1.0.0",
  "description": "Minimal runtime required for gamebundler",
  "main": "lib/index.js",
  "module": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "game",
    "bundler"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/pixi": {
      "import": {
        "types": "./lib/pixi.d.ts",
        "default": "./lib/pixi.js"
      },
      "require": {
        "types": "./lib/pixi.d.ts",
        "default": "./lib/pixi.js"
      }
    }
  },
  "devDependencies": {
    "@gamebundler/comptime": "^1.0.0",
    "@pixi/loaders": "^6.4.2",
    "@pixi/sound": "^4.2.0"
  }
}
